<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">


        <route method="POST" url="/V1/notifications/webhooks">
            <service class="PayPalBR\PayPalPlus\Api\WebHookManagementInterface" method="postWebHook"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
        </route>


        <route method="POST" url="/V1/notifications/notificationUrl">
            <service class="PayPalBR\PayPalPlus\Api\WebHookManagementInterface" method="notificationUrl"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
        </route>





</routes>
